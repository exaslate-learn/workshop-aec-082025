FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.78;	//0.24;

boundaryField
{

       top
    {
        type            fixedValue;
        value           $internalField;		//uniform 0.24;
    }
    
        bottom
    {
        type            inletOutlet;
        inletValue      $internalField;		//uniform 0.24;
        value           $internalField;		//uniform 0.24;
    }

        back
    {
        type            kqRWallFunction;
        value           $internalField;		//uniform 0.24;
    }

        RaceCar
    {
        type            kqRWallFunction;
        value           $internalField;		//uniform 0.24;
    }

        front
    {
        type           zeroGradient;
    }

    "(right|left)"
    {
        type           zeroGradient;
    }


    "proc.*"
    {
        type           processor;
    }
}


