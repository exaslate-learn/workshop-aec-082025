FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

flowVelocity    (0 -22.22 0);

pressure        0;

turbulentKE     0.24;

turbulentOmega  1.78;

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0); //(0 -22.22 0);	//-1); 

boundaryField
{
 
        top
    {
        type            fixedValue;
        value           uniform (0 -22.22 0);     
        
    }

     
        bottom
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 -22.22 0);
    }


        back
    {
        type        fixedValue;
        value       uniform (0 0 0);
    }

        RaceCar
    {
        type           noSlip;	//Slip;
    }
   
        right
    {
        type        zeroGradient;
        
    }

        left
    {
        type        zeroGradient;
 
    }

        front
    {
        type        zeroGradient;
    }


    "proc.*"
    {
        type       processor;
    }
}


