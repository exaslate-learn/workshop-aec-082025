FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.24;

boundaryField
{

       bottom
    {
        type            fixedValue;
        value           uniform 0.24;
    }
    
        top
    {
        type            inletOutlet;
        inletValue      uniform 0.24;
        value           uniform 0.24;
    }

        right
    {
        type            kqRWallFunction;
        value           uniform 0.24;
    }

        RaceCar
    {
        type            kqRWallFunction;
        value           uniform 0.24;
    }

        front
    {
        type           zeroGradient;
    }

    "(back|left)"
    {
        type           zeroGradient;
    }


    "proc.*"
    {
        type           processor;
    }
}


